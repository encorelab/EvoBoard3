package classes{	import flash.events.Event;	public class CustomEvent extends Event	{		public static const CLOSE:String = "Close";		public static const CLICK:String = "Click"; 		public static const EXPAND:String = "Expand"; 		public var customObject:*;				function CustomEvent( o:*, eventType:String )		{			customObject = o;			super( eventType, true ); 		}				public override function clone():Event 		{ 			return new CustomEvent ( customObject, type )		}	}}